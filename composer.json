{
    "description": "Extension for t3n/graphql that implements upload-handling",
    "type": "neos-package",
    "name": "t3n/graphql-upload",
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "neos/flow": "^7.0 || ^8.0 || dev-master",
        "t3n/graphql": "^3.0",
        "php": ">=7.3",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "t3n\\GraphQL\\Upload\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "t3n\\GraphQL\\Upload\\": "Classes/"
        }
    },
    "extra": {
        "neos": {
            "package-key": "t3n.GraphQL.Upload"
        }
    }
}
